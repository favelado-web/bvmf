{
  "name": "bvmf",
  "version": "0.8.0",
  "description": "Promise-based abstraction integrated with statusinvest.com.br for stock quotes",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebertcisco/bvmf.git"
  },
  "keywords": [
    "promise",
    "typescript",
    "quotes",
    "invest",
    "stock"
  ],
  "author": "Hebert Cisco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hebertcisco/bvmf/issues"
  },
  "homepage": "https://github.com/hebertcisco/bvmf#readme",
  "devDependencies": {
    "@types/cheerio": "0.22.30",
    "@types/jest": "27.4.0",
    "@types/node-fetch": "3.0.2",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "eslint": "7.11.0",
    "eslint-plugin-jest": "25.7.0",
    "jest": "27.4.7",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "node-fetch": "^3.0.0",
    "slugify": "^1.6.0"
  }
}
